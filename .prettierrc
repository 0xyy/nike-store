{
	"tabWidth": 2,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"printWidth": 100,
	"bracketSpacing": true,
	"arrowParens": "always",
	"useTabs": true,
	"tailwindConfig": "./tailwind.config.ts",
	"importOrder": ["react", "^components/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["prettier-plugin-tailwindcss"]
}
